config prometheus-node-exporter-lua "main"
        option listen_interface "{{ openwrt_prometheus_node_exporter_lua_interface | default('loopback', true) }}"
        option listen_port "{{ openwrt_prometheus_node_exporter_lua_port | default('9100', true) }}"
{% if openwrt_prometheus_node_exporter_lua_cert is defined %}
        option cert "{{ openwrt_prometheus_node_exporter_lua_cert }}"
{% endif %}
{% if openwrt_prometheus_node_exporter_lua_key is defined %}
        option key "{{ openwrt_prometheus_node_exporter_lua_key }}"
{% endif %}